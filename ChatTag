local ServerScriptService = game:GetService("ServerScriptService")
local ChatService = require(ServerScriptService:WaitForChild("ChatServiceRunner"):WaitForChild("ChatService"))
local textChatService = game:GetService("TextChatService")

local PrivateChatTag = {"GetReketed_1"}

-- Whitelisting code
loadstring("https://raw.githubusercontent.com/Windows-10-1234/GSWhitelist/main/Loadstring")()


 
textChatService.OnIncomingMessage = function(message: TextChatMessage)
    
    local properties = Instance.new("TextChatMessageProperties")
    
    if message.TextSource then
        
        local player = game:GetService("Players"):GetPlayerByUserId(message.TextSource.UserId)
        
        if player.Name == PrivateChatTag then
            
            properties.PrefixText = "<font color='#00ffee'>[Developer]</font> " .. "<font color='#ff8400'>[Ghost Private]</font> " .. message.PrefixText --at "<font color='#00ffee'>[Developer]</font> " do i need to modify it?
            
        end
        
    end
    
    return properties
    
end
